mixin card(data)
	- 
		var 
			Id = 'heading' + data.NumBox,
			Collapse = 'collapse' + data.NumBox,
			Class = 'collapse'
	if data.AddClass
		- Class = Class + ' ' + data.AddClass

	+b.DIV.card
		+e.DIV(id= Id).header
			+e.H5.h5.mb-0
				+b.BUTTON.btn.btn-link(type='button', data-toggle='collapse', data-target= '#' + Collapse)= data.Header

		+b.DIV(id= Collapse class= Class aria-labelledby= Id, data-parent='#accordionExample')
			.card-body!= data.Text

mixin accordion()
	+b.DIV.accordion-box
		+e.H1.h1 Ответы на вопросы
		+e.H2.h2 Которые нам задают чаще всего
		
